{
  "expo": {
    "name": "Badehy Mobile",
    "slug": "badehy-mobile",
    "scheme": "badehy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.anonymous.badehy-mobile"
    },
    "android": {
      "package": "com.badehy.mobile"
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "local"
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your trainer.",
          "cameraPermission": "The app accesses your camera to let you take photos to share with your trainer."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ]
  }
}
